[alias]
  c = commit -am
  up = pull
  p = push
  s = status -s
  df = diff --color --color-words --abbrev
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
[user]
  name = Michael Schieben
  email = babe@rockitbaby.de
[github]
  user = rockitbaby
  token = 
[core]
  excludesfiles = /.gitignore
  whitespace = fix,space-before-tab,tab-in-indent,trailing-space
[apply]
  whitespace = fix
[color]
  ui = always
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[merge]
  log = true
